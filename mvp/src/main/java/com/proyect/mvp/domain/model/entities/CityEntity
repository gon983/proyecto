import lombok.Getter;
import lombok.NoArgsConstructor;
import javax.persistence.*;

@Entity
@Table(name = "city")
@Getter
@NoArgsConstructor
public class City {

    @Id
    @Column(name = "id_city")
    private String idCity;

    @Column(name = "name")
    private String name;

    @ManyToOne
    @JoinColumn(name = "fk_country")
    private Country country; 
}